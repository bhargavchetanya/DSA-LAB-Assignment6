#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

int main() {
    int n, x;
    cin >> n;
    Node* head = nullptr;
    Node* tail = nullptr;
    for (int i = 0; i < n; i++) {
        cin >> x;
        Node* t = new Node{x, nullptr};
        if (!head) {
            head = tail = t;
            head->next = head;
        } else {
            tail->next = t;
            tail = t;
            tail->next = head;
        }
    }
    Node* p = head;
    do {
        cout << p->data << " ";
        p = p->next;
    } while (p != head);
    cout << head->data << "\n";
}
