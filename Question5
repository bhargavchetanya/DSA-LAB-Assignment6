#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

bool isCircular(Node* head) {
    if (!head) return false;
    Node* t = head->next;
    while (t && t != head) t = t->next;
    return t == head;
}

int main() {
    Node* n1 = new Node{2, nullptr};
    Node* n2 = new Node{4, nullptr};
    Node* n3 = new Node{6, nullptr};
    Node* n4 = new Node{7, nullptr};
    Node* n5 = new Node{5, nullptr};
    n1->next = n2; n2->next = n3; n3->next = n4; n4->next = n5; n5->next = n1;
    cout << (isCircular(n1) ? "True" : "False") << "\n";
}
